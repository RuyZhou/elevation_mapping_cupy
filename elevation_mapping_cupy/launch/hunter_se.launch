<launch>
    <arg name="use_sim_time" default="true"/>
    <!-- Elevation mapping node -->
    <node pkg="elevation_mapping_cupy" type="elevation_mapping_node" name="elevation_mapping" output="screen">
        <rosparam command="load" file="$(find elevation_mapping_cupy)/config/setups/hunter_se/hunter_se_parameters.yaml"/>
        <rosparam command="load" file="$(find elevation_mapping_cupy)/config/setups/hunter_se/hunter_se_sensor_parameter.yaml"/>
    </node>
</launch>
